{
  "name": "ts-template",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@7.12.0",
  "description": "",
  "author": "Cole <bernankez@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/bernankez/TSTemplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bernankez/TSTemplate.git"
  },
  "bugs": "https://github.com/bernankez/TSTemplate/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "npm run build",
    "release": "bumpp",
    "start": "tsx src/index.ts",
    "taze": "taze",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "linf:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.0.2",
    "@types/node": "^18.7.19",
    "bumpp": "^8.2.1",
    "changelogithub": "^0.12.4",
    "eslint": "^8.24.0",
    "rimraf": "^3.0.2",
    "taze": "^0.8.1",
    "tsx": "^3.9.0",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vite": "^3.1.3",
    "vitest": "^0.23.4"
  }
}
